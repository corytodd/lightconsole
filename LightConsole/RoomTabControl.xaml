<UserControl x:Class="LightConsole.RoomTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LightConsole"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="150">
    <Grid>
        <TabControl x:Name="tabcontrol" HorizontalContentAlignment="Stretch" 
                    VerticalContentAlignment="Stretch" TabStripPlacement="Right">
        </TabControl>
    </Grid>
</UserControl>
